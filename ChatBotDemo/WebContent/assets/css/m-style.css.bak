.imgContainer {position: relative;width: 100%;}
.turnImg {  background: url(../images/ico_360_50.png);  width:50px;height:50px;transition: .5s ease;  opacity: 0;  position: absolute;  top: 50%;  left: 50%;  transform: translate(-50%, -50%);  -ms-transform: translate(-50%, -50%)}
.playImg {  background: url(../images/ico_play_50.png);  width:50px;height:50px;transition: .5s ease;  opacity: 0;  position: absolute;  top: 50%;  left: 50%;  transform: translate(-50%, -50%);  -ms-transform: translate(-50%, -50%)}
.mapImg {  background: url("https://bottest.hyundai.com/assets/images/m_btn_maximazing_blue_50.png");  width:50px;height:50px;transition: .5s ease;  opacity: 0;  position: absolute;  top: 50%;  left: 50%;  transform: translate(-50%, -50%);  -ms-transform: translate(-50%, -50%)}
.imgImg {  background: url(../images/d_icn_img.png);  width:34px;height:34px;transition: .5s ease;  opacity: 0;  position: absolute;  top: 75%;  left: 85%;  transform: translate(-50%, -50%);  -ms-transform: translate(-50%, -50%)}

.imgContainer .turnImg {  opacity: 1;}
.imgContainer .playImg {  opacity: 1;}
.imgContainer .mapImg {  opacity: 1;}
.imgContainer .imgImg {  opacity: 1;}

