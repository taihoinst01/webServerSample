.popupArea{opacity:0;position: absolute;bottom: 0; width:100%;height:70%; z-index: 999999; border-radius: 18px 18px 0 0;}
.popupArea.popColor{background:#fbfbfb;}

.popHeader{background: #FFF;height: 33px;width: 100%; position: relative;border-radius:18px 18px 0 0; border-bottom: 1px #ccc solid; text-align: center;}
.popHeader > span{font-size: 14px;    color: #333;    font-weight: bold;    padding:9px 12px 0px;    display: inline-block;}
.popHeader > .popUpload{background: url(../images/ico_popup_upload_m.png) no-repeat; width:22px;height:22px;position: absolute; left: 4%;top: 7px;}
.popHeader > .popClose{background: url(../images/ico_popup_close_m.png) no-repeat; width:22px;height:22px;position: absolute; right: 4%;top: 7px;}
.popHeader > button{background: url(../images/ico_popup_close_m.png) no-repeat; width:22px;height:22px;position: absolute; right: 4%;top: 7px;}
.popBody{position: relative;border-top:1px solid rgba(0,0,0,0.20);}
.popTxt{text-align: center;    position: absolute;    bottom:11px; width: 100%; }
.popTxt > span{background-color:#bcbcbc; color: #FFF; border-radius: 5px; padding:4px 10px 5px;}
.popMask{background-color: #000; opacity: 0.5; height: 100%; width: 100%; z-index: 999; position: absolute; top:0; left: 0;}


.linkTest{color: #CCC; padding:10px; cursor: pointer;    position: absolute;    top: 10px;    left: 10px;}
